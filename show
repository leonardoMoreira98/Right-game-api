[32m+[m[32mimport type { HttpContextContract } from '@ioc:Adonis/Core/HttpContext'[m
[32m+[m[32mimport BadRequest from 'App/Exceptions/BadRequestException'[m
[32m+[m
[32m+[m[32mimport SportsCourtRequest from 'App/Models/SportsCourtRequest'[m
[32m+[m[32mimport { DateTime } from 'luxon'[m
[32m+[m
[32m+[m[32mexport default class SportsCourtRequestsController {[m
[32m+[m[32m  public async store({ request, response, auth }: HttpContextContract) {[m
[32m+[m[32m    const reservationTime = request.input('reservationTime')[m
[32m+[m
[32m+[m[32m    const sportsCourtId = request.param('sportsCourtId') as number[m
[32m+[m[32m    const userId = auth.user!.id[m
[32m+[m
[32m+[m[32m    const existing = await SportsCourtRequest.query()[m
[32m+[m[32m      .where('sportsCourtId', sportsCourtId)[m
[32m+[m[32m      .where('status', 'PENDING')[m
[32m+[m[32m      .andWhere('userId', userId)[m
[32m+[m[32m      .first()[m
[32m+[m
[32m+[m[32m    if (existing) throw new BadRequest('sports court request already exists', 409)[m
[32m+[m[32m    console.log(reservationTime)[m
[32m+[m[32m    const sportsCourtRequest = await SportsCourtRequest.create({[m
[32m+[m[32m      userId,[m
[32m+[m[32m      sportsCourtId,[m
[32m+[m[32m      reservationTime,[m
[32m+[m[32m    })[m
[32m+[m
[32m+[m[32m    await sportsCourtRequest.refresh()[m
[32m+[m
[32m+[m[32m    response.created({ sportsCourtRequest })[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  public async show({ request, response, bouncer }: HttpContextContract) {[m
[32m+[m[32m    const id = request.param('requestId') as number[m
[32m+[m[32m    const sportsCourtRequest = await SportsCourtRequest.findOrFail(id)[m
[32m+[m
[32m+[m[32m    response.ok({ sportsCourtRequest })[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  public async index({ response }: HttpContextContract) {[m
[32m+[m[32m    const sportsCourts = await SportsCourtRequest.query()[m
[32m+[m[32m      .where('status', 'PENDING')[m
[32m+[m[32m      .preload('user')[m
[32m+[m[32m      .preload('sportsCourt')[m
[32m+[m[32m    response.ok({ sportsCourts })[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  public async update({ request, response, bouncer }: HttpContextContract) {[m
[32m+[m[32m    const id = request.param('requestId') as number[m
[32m+[m[32m    const sportsCourtPayload = request.all()[m
[32m+[m
[32m+[m[32m    const sportsCourt = await SportsCourtRequest.findOrFail(id)[m
[32m+[m
[32m+[m[32m    const updatedSportsCourt = await sportsCourt.merge(sportsCourtPayload).save()[m
[32m+[m
[32m+[m[32m    response.ok({ sportsCourt: updatedSportsCourt })[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  public async destroy({ request, response, bouncer }: HttpContextContract) {[m
[32m+[m[32m    const id = request.param('requestId') as number[m
[32m+[m[32m    const sportsCourt = await SportsCourtRequest.findOrFail(id)[m
[32m+[m
[32m+[m[32m    await sportsCourt.delete()[m
[32m+[m
[32m+[m[32m    response.ok({})[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[1mdiff --git a/app/Controllers/Http/UsersController.ts b/app/Controllers/Http/UsersController.ts[m
[1mnew file mode 100644[m
[1mindex 0000000..f03c3d0[m
[1m--- /dev/null[m
[1m+++ b/app/Controllers/Http/UsersController.ts[m
[36m@@ -0,0 +1,35 @@[m
[32m+[m[32mimport type { HttpContextContract } from '@ioc:Adonis/Core/HttpContext'[m
[32m+[m[32mimport BadRequest from 'App/Exceptions/BadRequestException'[m
[32m+[m[32mimport User from 'App/Models/User'[m
[32m+[m[32mimport CreateUser from 'App/Validators/CreateUserValidator'[m
[32m+[m[32mimport UpdateUser from 'App/Validators/UpdateUserValidator'[m
[32m+[m
[32m+[m[32mexport default class UsersController {[m
[32m+[m[32m  public async store({ request, response }: HttpContextContract) {[m
[32m+[m[32m    const userPayload = await request.validate(CreateUser)[m
[32m+[m
[32m+[m[32m    const userByEmail = await User.findBy('email', userPayload.email)[m
[32m+[m[32m    const userByCpf = await User.findBy('cpf', userPayload.cpf)[m
[32m+[m
[32m+[m[32m    if (userByEmail) throw new BadRequest('email is already in use', 409)[m
[32m+[m[32m    if (userByCpf) throw new BadRequest('cpf is already in use', 409)[m
[32m+[m
[32m+[m[32m    const user = await User.create(userPayload)[m
[32m+[m[32m    response.created({ user })[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  public async update({ request, response, bouncer }: HttpContextContract) {[m
[32m+[m[32m    const { email, password } = await request.validate(UpdateUser)[m
[32m+[m[32m    const id = request.param('id')[m
[32m+[m[32m    const user = await User.findOrFail(id)[m
[32m+[m
[32m+[m[32m    await bouncer.authorize('updateUser', user)[m
[32m+[m
[32m+[m[32m    user.email = email[m
[32m+[m[32m    user.password = password[m
[32m+[m
[32m+[m[32m    await user.save()[m
[32m+[m
[32m+[m[32m    response.ok({ user })[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
