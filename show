[1m--- /dev/null[m
[1m+++ b/contracts/auth.ts[m
[36m@@ -0,0 +1,73 @@[m
[32m+[m[32m/**[m
[32m+[m[32m * Contract source: https://git.io/JOdz5[m
[32m+[m[32m *[m
[32m+[m[32m * Feel free to let us know via PR, if you find something broken in this[m
[32m+[m[32m * file.[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mimport User from 'App/Models/User'[m
[32m+[m
[32m+[m[32mdeclare module '@ioc:Adonis/Addons/Auth' {[m
[32m+[m[32m  /*[m
[32m+[m[32m  |--------------------------------------------------------------------------[m
[32m+[m[32m  | Providers[m
[32m+[m[32m  |--------------------------------------------------------------------------[m
[32m+[m[32m  |[m
[32m+[m[32m  | The providers are used to fetch users. The Auth module comes pre-bundled[m
[32m+[m[32m  | with two providers that are `Lucid` and `Database`. Both uses database[m
[32m+[m[32m  | to fetch user details.[m
[32m+[m[32m  |[m
[32m+[m[32m  | You can also create and register your own custom providers.[m
[32m+[m[32m  |[m
[32m+[m[32m  */[m
[32m+[m[32m  interface ProvidersList {[m
[32m+[m[32m    /*[m
[32m+[m[32m    |--------------------------------------------------------------------------[m
[32m+[m[32m    | User Provider[m
[32m+[m[32m    |--------------------------------------------------------------------------[m
[32m+[m[32m    |[m
[32m+[m[32m    | The following provider uses Lucid models as a driver for fetching user[m
[32m+[m[32m    | details from the database for authentication.[m
[32m+[m[32m    |[m
[32m+[m[32m    | You can create multiple providers using the same underlying driver with[m
[32m+[m[32m    | different Lucid models.[m
[32m+[m[32m    |[m
[32m+[m[32m    */[m
[32m+[m[32m    user: {[m
[32m+[m[32m      implementation: LucidProviderContract<typeof User>[m
[32m+[m[32m      config: LucidProviderConfig<typeof User>[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  /*[m
[32m+[m[32m  |--------------------------------------------------------------------------[m
[32m+[m[32m  | Guards[m
[32m+[m[32m  |--------------------------------------------------------------------------[m
[32m+[m[32m  |[m
[32m+[m[32m  | The guards are used for authenticating users using different drivers.[m
[32m+[m[32m  | The auth module comes with 3 different guards.[m
[32m+[m[32m  |[m
[32m+[m[32m  | - SessionGuardContract[m
[32m+[m[32m  | - BasicAuthGuardContract[m
[32m+[m[32m  | - OATGuardContract ( Opaque access token )[m
[32m+[m[32m  |[m
[32m+[m[32m  | Every guard needs a provider for looking up users from the database.[m
[32m+[m[32m  |[m
[32m+[m[32m  */[m
[32m+[m[32m  interface GuardsList {[m
[32m+[m[32m    /*[m
[32m+[m[32m    |--------------------------------------------------------------------------[m
[32m+[m[32m    | OAT Guard[m
[32m+[m[32m    |--------------------------------------------------------------------------[m
[32m+[m[32m    |[m
[32m+[m[32m    | OAT, stands for (Opaque access tokens) guard uses database backed tokens[m
[32m+[m[32m    | to authenticate requests.[m
[32m+[m[32m    |[m
[32m+[m[32m    */[m
[32m+[m[32m    api: {[m
[32m+[m[32m      implementation: OATGuardContract<'user', 'api'>[m
[32m+[m[32m      config: OATGuardConfig<'user'>[m
[32m+[m[32m      client: OATClientContract<'user'>[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
