[32m+[m[32mimport { schema, CustomMessages } from '@ioc:Adonis/Core/Validator'[m
[32m+[m[32mimport type { HttpContextContract } from '@ioc:Adonis/Core/HttpContext'[m
[32m+[m
[32m+[m[32mexport default class CreateSportsCourtValidator {[m
[32m+[m[32m  constructor(protected ctx: HttpContextContract) {}[m
[32m+[m
[32m+[m[32m  /*[m
[32m+[m[32m   * Define schema to validate the "shape", "type", "formatting" and "integrity" of data.[m
[32m+[m[32m   *[m
[32m+[m[32m   * For example:[m
[32m+[m[32m   * 1. The username must be of data type string. But then also, it should[m
[32m+[m[32m   *    not contain special characters or numbers.[m
[32m+[m[32m   *    ```[m
[32m+[m[32m   *     schema.string({}, [ rules.alpha() ])[m
[32m+[m[32m   *    ```[m
[32m+[m[32m   *[m
[32m+[m[32m   * 2. The email must be of data type string, formatted as a valid[m
[32m+[m[32m   *    email. But also, not used by any other user.[m
[32m+[m[32m   *    ```[m
[32m+[m[32m   *     schema.string({}, [[m
[32m+[m[32m   *       rules.email(),[m
[32m+[m[32m   *       rules.unique({ table: 'users', column: 'email' }),[m
[32m+[m[32m   *     ])[m
[32m+[m[32m   *    ```[m
[32m+[m[32m   */[m
[32m+[m[32m  public schema = schema.create({[m
[32m+[m[32m    name: schema.string(),[m
[32m+[m[32m    description: schema.string(),[m
[32m+[m[32m    location: schema.string(),[m
[32m+[m[32m    owner: schema.number(),[m
[32m+[m[32m  })[m
[32m+[m
[32m+[m[32m  /**[m
[32m+[m[32m   * Custom messages for validation failures. You can make use of dot notation `(.)`[m
[32m+[m[32m   * for targeting nested fields and array expressions `(*)` for targeting all[m
[32m+[m[32m   * children of an array. For example:[m
[32m+[m[32m   *[m
[32m+[m[32m   * {[m
[32m+[m[32m   *   'profile.username.required': 'Username is required',[m
[32m+[m[32m   *   'scores.*.number': 'Define scores as valid numbers'[m
[32m+[m[32m   * }[m
[32m+[m[32m   *[m
[32m+[m[32m   */[m
[32m+[m[32m  public messages: CustomMessages = {}[m
[32m+[m[32m}[m
