[1mdiff --git a/env.ts b/env.ts[m
[1mnew file mode 100644[m
[1mindex 0000000..3ac1661[m
[1m--- /dev/null[m
[1m+++ b/env.ts[m
[36m@@ -0,0 +1,24 @@[m
[32m+[m[32m/*[m
[32m+[m[32m|--------------------------------------------------------------------------[m
[32m+[m[32m| Validating Environment Variables[m
[32m+[m[32m|--------------------------------------------------------------------------[m
[32m+[m[32m|[m
[32m+[m[32m| In this file we define the rules for validating environment variables.[m
[32m+[m[32m| By performing validation we ensure that your application is running in[m
[32m+[m[32m| a stable environment with correct configuration values.[m
[32m+[m[32m|[m
[32m+[m[32m| This file is read automatically by the framework during the boot lifecycle[m
[32m+[m[32m| and hence do not rename or move this file to a different location.[m
[32m+[m[32m|[m
[32m+[m[32m*/[m
[32m+[m
[32m+[m[32mimport Env from '@ioc:Adonis/Core/Env'[m
[32m+[m
[32m+[m[32mexport default Env.rules({[m
[32m+[m[32m  HOST: Env.schema.string({ format: 'host' }),[m
[32m+[m[32m  PORT: Env.schema.number(),[m
[32m+[m[32m  APP_KEY: Env.schema.string(),[m
[32m+[m[32m  APP_NAME: Env.schema.string(),[m
[32m+[m[32m  DRIVE_DISK: Env.schema.enum(['local'] as const),[m
[32m+[m[32m  NODE_ENV: Env.schema.enum(['development', 'production', 'test'] as const),[m
