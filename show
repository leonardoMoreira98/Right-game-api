[32m+[m
[32m+[m[32mimport Env from '@ioc:Adonis/Core/Env'[m
[32m+[m[32mimport { hashConfig } from '@adonisjs/core/build/config'[m
[32m+[m
[32m+[m[32m/*[m
[32m+[m[32m|--------------------------------------------------------------------------[m
[32m+[m[32m| Hash Config[m
[32m+[m[32m|--------------------------------------------------------------------------[m
[32m+[m[32m|[m
[32m+[m[32m| The `HashConfig` relies on the `HashList` interface which is[m
[32m+[m[32m| defined inside `contracts` directory.[m
[32m+[m[32m|[m
[32m+[m[32m*/[m
[32m+[m[32mexport default hashConfig({[m
[32m+[m[32m  /*[m
[32m+[m[32m  |--------------------------------------------------------------------------[m
[32m+[m[32m  | Default hasher[m
[32m+[m[32m  |--------------------------------------------------------------------------[m
[32m+[m[32m  |[m
[32m+[m[32m  | By default we make use of the argon hasher to hash values. However, feel[m
[32m+[m[32m  | free to change the default value[m
[32m+[m[32m  |[m
[32m+[m[32m  */[m
[32m+[m[32m  default: Env.get('HASH_DRIVER', 'scrypt'),[m
[32m+[m
[32m+[m[32m  list: {[m
[32m+[m[32m    /*[m
[32m+[m[32m    |--------------------------------------------------------------------------[m
[32m+[m[32m    | scrypt[m
[32m+[m[32m    |--------------------------------------------------------------------------[m
[32m+[m[32m    |[m
[32m+[m[32m    | Scrypt mapping uses the Node.js inbuilt crypto module for creating[m
[32m+[m[32m    | hashes.[m
[32m+[m[32m    |[m
[32m+[m[32m    | We are using the default configuration recommended within the Node.js[m
[32m+[m[32m    | documentation.[m
[32m+[m[32m    | https://nodejs.org/api/crypto.html#cryptoscryptpassword-salt-keylen-options-callback[m
[32m+[m[32m    |[m
[32m+[m[32m    */[m
[32m+[m[32m    scrypt: {[m
[32m+[m[32m      driver: 'scrypt',[m
[32m+[m[32m      cost: 16384,[m
[32m+[m[32m      blockSize: 8,[m
[32m+[m[32m      parallelization: 1,[m
[32m+[m[32m      saltSize: 16,[m
[32m+[m[32m      keyLength: 64,[m
[32m+[m[32m      maxMemory: 32 * 1024 * 1024,[m
[32m+[m[32m    },[m
[32m+[m
[32m+[m[32m    /*[m
[32m+[m[32m    |--------------------------------------------------------------------------[m
[32m+[m[32m    | Argon[m
[32m+[m[32m    |--------------------------------------------------------------------------[m
[32m+[m[32m    |[m
[32m+[m[32m    | Argon mapping uses the `argon2` driver to hash values.[m
[32m+[m[32m    |[m
[32m+[m[32m    | Make sure you install the underlying dependency for this driver to work.[m
[32m+[m[32m    | https://www.npmjs.com/package/phc-argon2.[m
[32m+[m[32m    |[m
[32m+[m[32m    | npm install phc-argon2[m
[32m+[m[32m    |[m
[32m+[m[32m    */[m
[32m+[m[32m    argon: {[m
[32m+[m[32m      driver: 'argon2',[m
[32m+[m[32m      variant: 'id',[m
[32m+[m[32m      iterations: 3,[m
[32m+[m[32m      memory: 4096,[m
[32m+[m[32m      parallelism: 1,[m
[32m+[m[32m      saltSize: 16,[m
[32m+[m[32m    },[m
[32m+[m
[32m+[m[32m    /*[m
[32m+[m[32m    |--------------------------------------------------------------------------[m
[32m+[m[32m    | Bcrypt[m
[32m+[m[32m    |--------------------------------------------------------------------------[m
[32m+[m[32m    |[m
[32m+[m[32m    | Bcrypt mapping uses the `bcrypt` driver to hash values.[m
[32m+[m[32m    |[m
[32m+[m[32m    | Make sure you install the underlying dependency for this driver to work.[m
[32m+[m[32m    | https://www.npmjs.com/package/phc-bcrypt.[m
[32m+[m[32m    |[m
[32m+[m[32m    | npm install phc-bcrypt[m
[32m+[m[32m    |[m
[32m+[m[32m    */[m
[32m+[m[32m    bcrypt: {[m
[32m+[m[32m      driver: 'bcrypt',[m
[32m+[m[32m      rounds: 10,[m
[32m+[m[32m    },[m
[32m+[m[32m  },[m
[32m+[m[32m})[m
