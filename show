[1mdiff --git a/app/Controllers/Http/SessionsController.ts b/app/Controllers/Http/SessionsController.ts[m
[1mnew file mode 100644[m
[1mindex 0000000..d09766d[m
[1m--- /dev/null[m
[1m+++ b/app/Controllers/Http/SessionsController.ts[m
[36m@@ -0,0 +1,18 @@[m
[32m+[m[32mimport type { HttpContextContract } from '@ioc:Adonis/Core/HttpContext'[m
[32m+[m
[32m+[m[32mexport default class SessionsController {[m
[32m+[m[32m  public async store({ request, response, auth }: HttpContextContract) {[m
[32m+[m[32m    const { email, password } = request.only(['email', 'password'])[m
[32m+[m
[32m+[m[41m    [m
[32m+[m[32m    const token = await auth.use('api').attempt(email, password, {[m
[32m+[m[32m      expiresIn: '2hours',[m
[32m+[m[32m    })[m
[32m+[m
[32m+[m[32m    return response.created({ user: auth.user, token })[m
[32m+[m[32m  }[m
[32m+[m[32m  public async destroy({ response, auth }: HttpContextContract) {[m
[32m+[m[32m    await auth.logout()[m
[32m+[m[32m    return response.ok({})[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[1mdiff --git a/app/Controllers/Http/SportsCourtController.ts b/app/Controllers/Http/SportsCourtController.ts[m
[1mnew file mode 100644[m
[1mindex 0000000..de12b6b[m
[1m--- /dev/null[m
[1m+++ b/app/Controllers/Http/SportsCourtController.ts[m
[36m@@ -0,0 +1,49 @@[m
[32m+[m[32mimport type { HttpContextContract } from '@ioc:Adonis/Core/HttpContext'[m
[32m+[m[32mimport SportsCourt from 'App/Models/SportsCourt'[m
[32m+[m[32mimport CreateSportsCourt from 'App/Validators/CreateSportsCourtValidator'[m
[32m+[m
[32m+[m[32mexport default class SportsCourtController {[m
[32m+[m[32m  public async store({ request, response }: HttpContextContract) {[m
[32m+[m[32m    const sportsCourtPayload = await request.validate(CreateSportsCourt)[m
[32m+[m[32m    const sportsCourt = await SportsCourt.create(sportsCourtPayload)[m
[32m+[m
[32m+[m[32m    response.created({ sportsCourt })[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  public async index({ response }: HttpContextContract) {[m
[32m+[m[32m    const sportsCourts = await SportsCourt.query().preload('ownerUser')[m
[32m+[m[32m    response.ok({ sportsCourts })[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  public async getAllQuadras({ response }: HttpContextContract) {[m
[32m+[m[32m    const sportsCourts = await SportsCourt.query()[m
[32m+[m[32m    response.ok({ sportsCourts })[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  public async update({ request, response, bouncer }: HttpContextContract) {[m
[32m+[m[32m    const id = request.param('sportsCourtId') as number[m
[32m+[m[32m    const sportsCourtPayload = request.all()[m
[32m+[m
[32m+[m[32m    const sportsCourt = await SportsCourt.findOrFail(id)[m
[32m+[m
[32m+[m[32m    const updatedSportsCourt = await sportsCourt.merge(sportsCourtPayload).save()[m
[32m+[m
[32m+[m[32m    response.ok({ sportsCourt: updatedSportsCourt })[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  public async destroy({ request, response, bouncer }: HttpContextContract) {[m
[32m+[m[32m    const id = request.param('sportsCourtId') as number[m
[32m+[m[32m    const sportsCourt = await SportsCourt.findOrFail(id)[m
[32m+[m
[32m+[m[32m    await sportsCourt.delete()[m
[32m+[m
[32m+[m[32m    response.ok({})[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  public async show({ request, response, bouncer }: HttpContextContract) {[m
[32m+[m[32m    const id = request.param('sportsCourtId') as number[m
[32m+[m[32m    const sportsCourt = await SportsCourt.findOrFail(id)[m
[32m+[m
[32m+[m[32m    response.ok({ sportsCourt })[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
