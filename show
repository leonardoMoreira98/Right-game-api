[32m+[m[32mimport type { HttpContextContract } from '@ioc:Adonis/Core/HttpContext'[m
[32m+[m[32mimport { Exception } from '@adonisjs/core/build/standalone'[m
[32m+[m
[32m+[m[32m/*[m
[32m+[m[32m|--------------------------------------------------------------------------[m
[32m+[m[32m| Exception[m
[32m+[m[32m|--------------------------------------------------------------------------[m
[32m+[m[32m|[m
[32m+[m[32m| The Exception class imported from `@adonisjs/core` allows defining[m
[32m+[m[32m| a status code and error code for every exception.[m
[32m+[m[32m|[m
[32m+[m[32m| @example[m
[32m+[m[32m| new BadRequestException('message', 500, 'E_RUNTIME_EXCEPTION')[m
[32m+[m[32m|[m
[32m+[m[32m*/[m
[32m+[m[32mexport default class BadRequestException extends Exception {[m
[32m+[m[32m  public code = 'BAD_REQUEST'[m
[32m+[m
[32m+[m[32m  public async handle(error: this, ctx: HttpContextContract) {[m
[32m+[m[32m    return ctx.response[m
[32m+[m[32m      .status(error.status)[m
[32m+[m[32m      .send({ code: error.code, message: error.message, status: error.status })[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[1mdiff --git a/app/Exceptions/Handler.ts b/app/Exceptions/Handler.ts[m
[1mnew file mode 100644[m
[1mindex 0000000..226dbfe[m
[1m--- /dev/null[m
[1m+++ b/app/Exceptions/Handler.ts[m
[36m@@ -0,0 +1,48 @@[m
[32m+[m[32mimport { HttpContextContract } from '@ioc:Adonis/Core/HttpContext'[m
[32m+[m[32m/*[m
[32m+[m[32m|--------------------------------------------------------------------------[m
[32m+[m[32m| Http Exception Handler[m
[32m+[m[32m|--------------------------------------------------------------------------[m
[32m+[m[32m|[m
[32m+[m[32m| AdonisJs will forward all exceptions occurred during an HTTP request to[m
[32m+[m[32m| the following class. You can learn more about exception handling by[m
[32m+[m[32m| reading docs.[m
[32m+[m[32m|[m
[32m+[m[32m| The exception handler extends a base `HttpExceptionHandler` which is not[m
[32m+[m[32m| mandatory, however it can do lot of heavy lifting to handle the errors[m
[32m+[m[32m| properly.[m
[32m+[m[32m|[m
[32m+[m[32m*/[m
[32m+[m
[32m+[m[32mimport Logger from '@ioc:Adonis/Core/Logger'[m
[32m+[m[32mimport HttpExceptionHandler from '@ioc:Adonis/Core/HttpExceptionHandler'[m
[32m+[m[32mimport { Exception } from '@adonisjs/core/build/standalone'[m
[32m+[m
[32m+[m[32mexport default class ExceptionHandler extends HttpExceptionHandler {[m
[32m+[m[32m  constructor() {[m
[32m+[m[32m    super(Logger)[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  public async handle(error: Exception, ctx: HttpContextContract) {[m
[32m+[m[32m    if (error.status === 422)[m
[32m+[m[32m      return ctx.response.status(error.status).send({[m
[32m+[m[32m        code: 'BAD_REQUEST',[m
[32m+[m[32m        message: error.message,[m
[32m+[m[32m        status: error.status,[m
[32m+[m[32m        erros: error['messages']?.errors ? error['messages'].errors : '',[m
[32m+[m[32m      })[m
[32m+[m[32m    else if (error.code === 'E_ROW_NOT_FOUND')[m
[32m+[m[32m      return ctx.response.status(error.status).send({[m
[32m+[m[32m        code: 'BAD_REQUEST',[m
[32m+[m[32m        message: 'the requested resource was not found',[m
[32m+[m[32m        status: 404,[m
[32m+[m[32m      })[m
[32m+[m[32m    else if (['E_INVALID_AUTH_UID', 'E_INVALID_AUTH_PASSWORD'].includes(error.code || ''))[m
[32m+[m[32m      return ctx.response.status(error.status).send({[m
[32m+[m[32m        code: 'BAD_REQUEST',[m
[32m+[m[32m        message: 'invalid credentials',[m
[32m+[m[32m        status: 400,[m
[32m+[m[32m      })[m
[32m+[m[32m    return super.handle(error, ctx)[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[1mdiff --git a/app/Middleware/Auth.ts b/app/Middleware/Auth.ts[m
[1mnew file mode 100644[m
[1mindex 0000000..1a26ad0[m
[1m--- /dev/null[m
[1m+++ b/app/Middleware/Auth.ts[m
[36m@@ -0,0 +1,76 @@[m
[32m+[m[32mimport { AuthenticationException } from '@adonisjs/auth/build/standalone'[m
[32m+[m[32mimport type { GuardsList } from '@ioc:Adonis/Addons/Auth'[m
[32m+[m[32mimport type { HttpContextContract } from '@ioc:Adonis/Core/HttpContext'[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Auth middleware is meant to restrict un-authenticated access to a given route[m
[32m+[m[32m * or a group of routes.[m
[32m+[m[32m *[m
[32m+[m[32m * You must register this middleware inside `start/kernel.ts` file under the list[m
[32m+[m[32m * of named middleware.[m
[32m+[m[32m */[m
[32m+[m[32mexport default class AuthMiddleware {[m
[32m+[m[32m  /**[m
[32m+[m[32m   * The URL to redirect to when request is Unauthorized[m
[32m+[m[32m   */[m
[32m+[m[32m  protected redirectTo = '/login'[m
[32m+[m
[32m+[m[32m  /**[m
[32m+[m[32m   * Authenticates the current HTTP request against a custom set of defined[m
[32m+[m[32m   * guards.[m
[32m+[m[32m   *[m
[32m+[m[32m   * The authentication loop stops as soon as the user is authenticated using any[m
[32m+[m[32m   * of the mentioned guards and that guard will be used by the rest of the code[m
[32m+[m[32m   * during the current request.[m
[32m+[m[32m   */[m
[32m+[m[32m  protected async authenticate(auth: HttpContextContract['auth'], guards: (keyof GuardsList)[]) {[m
[32m+[m[32m    /**[m
[32m+[m[32m     * Hold reference to the guard last attempted within the for loop. We pass[m
[32m+[m[32m     * the reference of the guard to the "AuthenticationException", so that[m
[32m+[m[32m     * it can decide the correct response behavior based upon the guard[m
[32m+[m[32m     * driver[m
[32m+[m[32m     */[m
[32m+[m[32m    let guardLastAttempted: string | undefined[m
[32m+[m
[32m+[m[32m    for (let guard of guards) {[m
[32m+[m[32m      guardLastAttempted = guard[m
[32m+[m
[32m+[m[32m      if (await auth.use(guard).check()) {[m
[32m+[m[32m        /**[m
[32m+[m[32m         * Instruct auth to use the given guard as the default guard for[m
[32m+[m[32m         * the rest of the request, since the user authenticated[m
[32m+[m[32m         * succeeded here[m
[32m+[m[32m         */[m
[32m+[m[32m        auth.defaultGuard = guard[m
[32m+[m[32m        return true[m
[32m+[m[32m      }[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    /**[m
[32m+[m[32m     * Unable to authenticate using any guard[m
[32m+[m[32m     */[m
[32m+[m[32m    throw new AuthenticationException([m
[32m+[m[32m      'Unauthorized access',[m
[32m+[m[32m      'E_UNAUTHORIZED_ACCESS',[m
[32m+[m[32m      guardLastAttempted,[m
[32m+[m[32m      this.redirectTo,[m
[32m+[m[32m    )[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  /**[m
[32m+[m[32m   * Handle request[m
[32m+[m[32m   */[m
[32m+[m[32m  public async handle ([m
[32m+[m[32m    { auth }: HttpContextContract,[m
[32m+[m[32m    next: () => Promise<void>,[m
[32m+[m[32m    customGuards: (keyof GuardsList)[][m
[32m+[m[32m  ) {[m
[32m+[m[32m    /**[m
[32m+[m[32m     * Uses the user defined guards or the default guard mentioned in[m
[32m+[m[32m     * the config file[m
[32m+[m[32m     */[m
[32m+[m[32m    const guards = customGuards.length ? customGuards : [auth.name][m
[32m+[m[32m    await this.authenticate(auth, guards)[m
[32m+[m[32m    await next()[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[1mdiff --git a/app/Middleware/SilentAuth.ts b/app/Middleware/SilentAuth.ts[m
[1mnew file mode 100644[m
[1mindex 0000000..5d3ac8f[m
[1m--- /dev/null[m
[1m+++ b/app/Middleware/SilentAuth.ts[m
[36m@@ -0,0 +1,21 @@[m
[32m+[m[32mimport type { HttpContextContract } from '@ioc:Adonis/Core/HttpContext'[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Silent auth middleware can be used as a global middleware to silent check[m
[32m+[m[32m * if the user is logged-in or not.[m
[32m+[m[32m *[m
[32m+[m[32m * The request continues as usual, even when the user is not logged-in.[m
[32m+[m[32m */[m
[32m+[m[32mexport default class SilentAuthMiddleware {[m
[32m+[m[32m  /**[m
[32m+[m[32m   * Handle request[m
[32m+[m[32m   */[m
[32m+[m[32m  public async handle({ auth }: HttpContextContract, next: () => Promise<void>) {[m
[32m+[m[32m    /**[m
[32m+[m[32m     * Check if user is logged-in or not. If yes, then `ctx.auth.user` will be[m
[32m+[m[32m     * set to the instance of the currently logged in user.[m
[32m+[m[32m     */[m
[32m+[m[32m    await auth.check()[m
[32m+[m[32m    await next()[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
