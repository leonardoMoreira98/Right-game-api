[32m+[m[32mimport { listDirectoryFiles } from '@adonisjs/core/build/standalone'[m
[32m+[m[32mimport Application from '@ioc:Adonis/Core/Application'[m
[32m+[m
[32m+[m[32m/*[m
[32m+[m[32m|--------------------------------------------------------------------------[m
[32m+[m[32m| Exporting an array of commands[m
[32m+[m[32m|--------------------------------------------------------------------------[m
[32m+[m[32m|[m
[32m+[m[32m| Instead of manually exporting each file from this directory, we use the[m
[32m+[m[32m| helper `listDirectoryFiles` to recursively collect and export an array[m
[32m+[m[32m| of filenames.[m
[32m+[m[32m|[m
[32m+[m[32m| Couple of things to note:[m
[32m+[m[32m|[m
[32m+[m[32m| 1. The file path must be relative from the project root and not this directory.[m
[32m+[m[32m| 2. We must ignore this file to avoid getting into an infinite loop[m
[32m+[m[32m|[m
[32m+[m[32m*/[m
[32m+[m[32mexport default listDirectoryFiles(__dirname, Application.appRoot, ['./commands/index'])[m
