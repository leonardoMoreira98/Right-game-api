[32m+[m[32mimport Hash from '@ioc:Adonis/Core/Hash'[m
[32m+[m[32mimport { BaseModel, beforeSave, column } from '@ioc:Adonis/Lucid/Orm'[m
[32m+[m[32mimport { DateTime } from 'luxon'[m
[32m+[m
[32m+[m[32mexport default class User extends BaseModel {[m
[32m+[m[32m  @column({ isPrimary: true })[m
[32m+[m[32m  public id: number[m
[32m+[m
[32m+[m[32m  @column()[m
[32m+[m[32m  public name: string[m
[32m+[m
[32m+[m[32m  @column()[m
[32m+[m[32m  public email: string[m
[32m+[m
[32m+[m[32m  @column()[m
[32m+[m[32m  public cpf: number[m
[32m+[m
[32m+[m[32m  @column({ serializeAs: null })[m
[32m+[m[32m  public password: string[m
[32m+[m
[32m+[m[32m  @column.dateTime()[m
[32m+[m[32m  public birthday: DateTime[m
[32m+[m
[32m+[m[32m  @column()[m
[32m+[m[32m  public phoneNumber: string[m
[32m+[m
[32m+[m[32m  @column.dateTime({ autoCreate: true })[m
[32m+[m[32m  public createdAt: DateTime[m
[32m+[m
[32m+[m[32m  @column.dateTime({ autoCreate: true, autoUpdate: true })[m
[32m+[m[32m  public updatedAt: DateTime[m
[32m+[m
[32m+[m[32m  @beforeSave()[m
[32m+[m[32m  public static async hashPassword(user: User) {[m
[32m+[m[32m    if (user.$dirty.password) user.password = await Hash.make(user.password)[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
