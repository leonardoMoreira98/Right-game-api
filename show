[32m+[m[32mimport Factory from '@ioc:Adonis/Lucid/Factory'[m
[32m+[m[32mimport User from 'App/Models/User'[m
[32m+[m[32mimport SportsCourt from 'App/Models/SportsCourt'[m
[32m+[m[32mimport { DateTime } from 'luxon'[m
[32m+[m
[32m+[m[32mexport const UserFactory = Factory.define(User, ({ faker }) => {[m
[32m+[m[32m  return {[m
[32m+[m[32m    name: faker.person.firstName(),[m
[32m+[m[32m    cpf: faker.number.int(11),[m
[32m+[m[32m    password: faker.internet.password(),[m
[32m+[m[32m    email: faker.internet.email(),[m
[32m+[m[32m    birthday: DateTime.fromFormat('08-05-1999', 'dd-MM-yyyy'),[m
[32m+[m[32m    phoneNumber: faker.phone.number('+55 41 #####-####'),[m
[32m+[m[32m  }[m
[32m+[m[32m}).build()[m
[32m+[m
[32m+[m[32mexport const SportsCourtFactory = Factory.define(SportsCourt, ({ faker }) => {[m
[32m+[m[32m  return {[m
[32m+[m[32m    name: faker.company.name(),[m
[32m+[m[32m    description: faker.company.buzzVerb(),[m
[32m+[m[32m    location: faker.location.country(),[m
[32m+[m[32m  }[m
[32m+[m[32m}).build()[m
